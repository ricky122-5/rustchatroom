{
  "title": "Rustchatroom Overview",
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Connected Peers",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "expr": "sum(p2p_node_peers)",
          "legendFormat": "peers"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      }
    },
    {
      "id": 2,
      "title": "Gossip Traffic",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "expr": "sum(rate(p2p_node_gossip_published[1m]))",
          "legendFormat": "published"
        },
        {
          "expr": "sum(rate(p2p_node_gossip_received[1m]))",
          "legendFormat": "received"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "msgs/s"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      }
    },
    {
      "id": 3,
      "title": "Ping Latency",
      "type": "timeseries",
      "datasource": {
        "type": "prometheus",
        "uid": "PROMETHEUS_DS"
      },
      "targets": [
        {
          "expr": "avg(p2p_node_ping_latency_ms)",
          "legendFormat": "latency"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms"
        },
        "overrides": []
      },
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 8
      }
    }
  ]
}
